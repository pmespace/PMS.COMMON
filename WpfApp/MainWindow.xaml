        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Button x:Name="press" Content="Button" HorizontalAlignment="Left" Margin="65,48,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <TextBox x:Name="efBox" HorizontalAlignment="Left" Margin="128,49,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="reset" Content="Button" HorizontalAlignment="Left" Margin="281,49,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>

    </Grid>
